{
   "servers":{
      "rust":{
         "command":[
            "rust-analyzer"
         ],
         "path":[
            "%{ENV:CARGO_HOME}/bin",
            "%{ENV:HOME}/.cargo/bin",
            "%{ENV:USERPROFILE}/.cargo/bin"
         ],
         "url":"https://rust-analyzer.github.io",
         "highlightingModeRegex":"^Rust$",
         "initializationOptions":{
            "check":{
	     "command":"clippy"
            }
         }
      }
   }
}