[
  {
    "bindings": {
      "ctrl-w": "zed::NoAction",
      "ctrl-w ctrl-w": "pane::CloseActiveItem",
      "alt-b": "workspace::ToggleLeftDock",
      "alt-m": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "ctrl-w": "zed::NoAction",
      "ctrl-w c": "pane::CloseActiveItem",
      "shift-enter": "editor::NewlineAbove",
    }
  },
  {
    "context": "vim_mode == normal",
    "bindings": {
      "tab": "editor::Format",
      "ctrl-k ctrl-o": "workspace::Open"
    }
  }
]
